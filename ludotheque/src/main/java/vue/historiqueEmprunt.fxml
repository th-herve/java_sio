<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="480.0"
	prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controleur.scene.HistoriqueEmpruntControleur">
	<top>
		<Label text="Historique des emprunts"
			BorderPane.alignment="CENTER">
			<font>
				<Font size="31.0" />
			</font>
		</Label>
	</top>
	<center>
		<TableView fx:id="tableHistoriqueEmprunt" prefHeight="200.0"
			prefWidth="200.0" BorderPane.alignment="CENTER">
			<columns>
				<TableColumn fx:id="idJeuPhysique" prefWidth="65.0"
					text="Id Jeux" />
				<TableColumn fx:id="nomJeuPhysique" prefWidth="69.0"
					text="Nom Jeux" />
				<TableColumn fx:id="idAdherent" prefWidth="102.0"
					text="Id Adherent" />
				<TableColumn fx:id="nomAdherent" prefWidth="75.0"
					text="Nom Adherent" />
				<TableColumn fx:id="dateEmprunt" prefWidth="75.0"
					text="Date d'emprunt" />
				<TableColumn fx:id="dateRetour" prefWidth="86.0"
					text="Date de retour" />
			</columns>
			<BorderPane.margin>
				<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
			</BorderPane.margin>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TableView>
	</center>
	<bottom>
		<HBox prefHeight="47.0" prefWidth="640.0"
			BorderPane.alignment="CENTER">
			<children>
				<Button mnemonicParsing="false" text="Supprimer" />
				<TextField />
			</children>
		</HBox>
		<Button mnemonicParsing="false" onAction="#switchToAccueil"
			text="Retour Ã  la page d'accueil">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
	</bottom>
</BorderPane>
